-- создаем таблицу опции отеля
CREATE TABLE HOTEL_OPTION (
ID_HOTEL BIGINT NOT NULL ,
TYPE VARCHAR(45) NOT NULL,
RATING_HOTEL VARCHAR(45),
NUMBER_OF_STARS VARCHAR(45),
GENIUS_SALE VARCHAR(45) NOT NULL
  )
-- добавляем содержимое таблицы опции отеля
INSERT INTO HOTEL_OPTION SELECT '1', 'HOSTEL', '9', NULL, 'YES'
INSERT INTO HOTEL_OPTION SELECT '2', 'HOSTEL', '7.1', NULL, 'NO'
INSERT INTO HOTEL_OPTION SELECT '3', 'HOTEL', NULL, '4', 'YES'
