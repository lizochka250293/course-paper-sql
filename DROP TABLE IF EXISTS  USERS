-- создаем таблицу каталог отелей
DROP TABLE IF EXISTS HOTEL_CATALOG
CREATE TABLE HOTEL_CATALOG (
ID_HOTEL BIGINT,
NAME_HOTEL VARCHAR(100) NOT NULL
)
