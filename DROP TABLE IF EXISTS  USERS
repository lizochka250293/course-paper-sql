-- создаем таблицу пользователи
DROP TABLE IF EXISTS  USERS 
CREATE TABLE  USERS  (
  USER_LOGIN varchar(100) not null ,
   USER_DATE  DATETIME not null,
  USER_ID  bigint not null 
) 
-- добавляем содержимое таблицы пользователи
INSERT INTO  USERS select 'abshbd@mail.ru', '2012-09-12 12:32:42', '1'
INSERT INTO  USERS  select 'gaydbh232@ya.ru', '2009-12-01 02:15:30', '2'
INSERT INTO  USERS select 'csdnicus76@mail.ru', '2013-05-06 15:30:12', '3'
INSERT INTO  USERS  select 'dwdc@mail.ru', '2019-11-08 17:12:09', 4
-- создаем таблицу карточка пользователя
DROP TABLE IF EXISTS USER_CARD 
CREATE TABLE  USER_CARD  (
  USER_ID    bigint ,
   USER_LOGIN  varchar(45) NOT NULL,
  USER_PASSWORD varchar(45) NOT NULL,
USER_FIST_NAME varchar(45) NOT NULL,
USER_LAST_NAME VARCHAR(45) NOT NULL,
USER_STATUS VARCHAR(45)
  )
-- добавляем содержимое таблицы карточка пользователя
INSERT INTO USER_CARD select 1, 'abshbd@mail.ru', 'sadf453nb', 'ILIA', 'IVANOV','1'
INSERT INTO USER_CARD SELECT  2, 'gaydbh232@ya.ru', 'Tmvd3432', 'IRINA', 'PETROVA', '2'
INSERT INTO USER_CARD SELECT 3, 'csdnicus76@mail.ru', 'KIhhe462jh', 'MARINA', 'SIDOROVA', NULL
INSERT INTO USER_CARD SELECT 4, 'dwdc@mail.ru', 'csh435jhy', 'MAXIM', 'BELKIN', NULL
-- создаем таблицу статус пользователя
DROP TABLE IF EXISTS  GENIUS_STATUS 
CREATE TABLE GENIUS_STATUS (
   STATUS  varchar(9) ,
  SALE BIGINT
)
-- добавляем содержимое таблицы статус пользователя
INSERT INTO GENIUS_STATUS SELECT  '1','10'
INSERT INTO GENIUS_STATUS SELECT '2','15'

-- создаем таблицу данные пользователя
DROP TABLE IF EXISTS USERS_QUESTIONNAIRE
CREATE TABLE USERS_QUESTIONNAIRE (
USER_ID BIGINT NOT NULL,
USER_CALL_NUMBER VARCHAR(45) not null,
USER_MAIL VARCHAR(45) NOT NULL,
USER_DATE_OF_BIRTH VARCHAR(45),
USER_SEX varchar(45),
USER_COUTRY VARCHAR(45) NOT NULL,
USER_CITY VARCHAR(45)
 )
 -- добавляем содержимое таблицы данные пользователя
