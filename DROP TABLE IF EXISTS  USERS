-- создаем таблицу сохраненные варианты
DROP TABLE IF EXISTS SAVED_OPTION
CREATE TABLE SAVED_OPTION (
USER_ID BIGINT NOT NULL ,
ID_HOTEL BIGINT  NOT NULL 
)
-- добавляем содержимое таблицы сохраненные варианты
INSERT INTO SAVED_OPTION SELECT '1','1'
INSERT INTO SAVED_OPTION SELECT '3','10'

-- создаем таблицу забронированные пользователем
DROP TABLE IF EXISTS BOOKING_OPTION
CREATE TABLE BOOKING_OPTION (
USER_ID BIGINT NOT NULL,
ID_HOTEL BIGINT NOT NULL,
BOOKING_ID BIGINT NOT NULL,
BOOKING_DATE DATETIME NOT NULL
)
-- добавляем содержимое таблицы забронированные пользователем
INSERT INTO BOOKING_OPTION SELECT '4','2','1', '2021-09-05 10:56:03'
INSERT INTO BOOKING_OPTION SELECT '2','3','2', '2020-04-15 22:58:02'
INSERT INTO BOOKING_OPTION SELECT '2','4','3', '2020-05-19 20:03:08'
INSERT INTO BOOKING_OPTION SELECT '3','8', '4','2021-06-15 08:03:54'

-- создаем таблицу подтвержденные отелем бронирования
DROP TABLE IF EXISTS CONFIRMED_BOOKING
CREATE TABLE CONFIRMED_BOOKING (
BOOKING_ID bigint NOT NULL,
ID_HOTEL bigint NOT NULL,
CONFIRMED_DATE datetime  NOT NULL
 )
 -- добавляем содержимое таблицы подтвержденные отелем бронирования
INSERT INTO CONFIRMED_BOOKING select '1','2', '2021-09-05 21:03:43'
INSERT INTO CONFIRMED_BOOKING select '2','3', '2020-04-16 11:24:57'
INSERT INTO CONFIRMED_BOOKING select '4','8', '2021-06-15 23:20:05'

