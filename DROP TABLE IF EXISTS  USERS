-- создаем таблицу карточка отеля
DROP TABLE IF EXISTS HOTEL_CARD
CREATE TABLE HOTEL_CARD (
ID_HOTEL BIGINT NOT NULL,
COUTRY VARCHAR(45) NOT NULL,
LOCALITY VARCHAR(45) NOT NULL,
  COST BIGINT NOT NULL,
)
