-- хранимые процедуры
-- заполнение таблицы данные пользователя
CREATE PROCEDURE INS_ USERS_ QUESTIONNAIRE (C CHAR(45), M CHAR(45), D CHAR(45), S CHAR(45), CO CHAR(45), CI CHAR(45))
BEGIN
INSERT INTO USERS_ QUESTIONNAIRE (USER_CALL_NUMBER, USER_MAIL, USER_DATE_OF_BIRTH, USER_SEX, USER_COUTRY, USER_CITY) VALUE (C, M, D, S, CO, CI);
END
//
